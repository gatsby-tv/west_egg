{
  "name": "westegg",
  "version": "0.0.2",
  "description": "The place where Gatsby lives.",
  "main": "dist/index.js",
  "scripts": {
    "format:check": "prettier --check \"./src/**/*.ts\"",
    "format:write": "prettier --write \"./src/**/*.ts\"",
    "format:test:check": "prettier --check \"./spec/**/*.ts\"",
    "format:test:write": "prettier --write \"./spec/**/*.ts\"",
    "build": "rm -rf ./dist && npm run format:write && ttsc",
    "start": "npm run build && node .",
    "test": "npm run format:test:write && mocha -r ts-node/register spec/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsby-tv/westegg.git"
  },
  "author": "",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/gatsby-tv/westegg/issues"
  },
  "homepage": "https://github.com/gatsby-tv/westegg#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/busboy": "^0.2.3",
    "@types/chai": "^4.2.19",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.2",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.6.4",
    "@types/redis": "^2.8.28",
    "@types/validator": "^13.1.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^9.0.1",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@gatsby-tv/types": "0.4.33",
    "@sendgrid/mail": "^7.4.2",
    "busboy": "^0.3.1",
    "core-js": "^3.7.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ipfs-http-client": "^48.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "redis": "^3.1.2",
    "ts-transformer-keys": "^0.4.3",
    "validator": "^13.1.17",
    "winston": "^3.3.3"
  }
