{
  "name": "westegg",
  "version": "1.0.0",
  "description": "The place where Gatsby lives.",
  "main": "dist/index.js",
  "scripts": {
    "format:check": "prettier --check \"./src/**/*.ts\"",
    "format:write": "prettier --write \"./src/**/*.ts\"",
    "build": "rm -rf ./dist && npm run format:write && tsc",
    "start": "npm run build && node .",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsby-tv/westegg.git"
  },
  "author": "",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/gatsby-tv/westegg/issues"
  },
  "homepage": "https://github.com/gatsby-tv/westegg#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.6.4",
    "@types/validator": "^13.1.0",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@gatsby-tv/types": "^0.4.4",
    "body-parser": "^1.19.0",
    "core-js": "^3.8.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ipfs-http-client": "^48.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "validator": "^13.1.17",
    "winston": "^3.3.3"
  }
}
